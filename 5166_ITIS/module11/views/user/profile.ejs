
<%- include('../partials/header.ejs')%>
<div class = "content">
    <h2>Welcome, <%=user.firstName%></h2>
    <%if(stories.length) {%>
        <p>You have created the following stories</p>
        <%stories.forEach(story => {%>
            <a href = '/stories/<%= story.id%>'><p><%=story.title%></p></a>
        <%});
    } else {%>
        <p>You have not created any stories</p>
    <%}%>
    </div>
